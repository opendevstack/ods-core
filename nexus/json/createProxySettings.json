{
  "name":"createProxySettings",
  "content":"String httpProxy = '@http_proxy@';
  if (httpProxy.trim().length() > 0) {
    String[] hostAndPort = httpProxy.split (':');
    int httpProxyPort = 80;
    String httpProxyName = hostAndPort[0];
    if (httpProxy.contains(':')) {
        log.info("port")
    }
    core.httpProxy(httpProxyName, httpProxyPort);
  }",
 "type":"groovy"
}
